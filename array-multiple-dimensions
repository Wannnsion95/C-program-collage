#include <stdio.h>
int main() {
    
    int matriks[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };
    
    int array3D[2][2][2] = {
        {{1, 2}, {3, 4}},
        {{5, 6}, {7, 8}}
    };

    printf("Array 2 Dimensi (Matriks 3x3):\n");
    for(int i = 0; i < 3; i++) {
        for(int j = 0; j < 3; j++) {
            printf("%d ", matriks[i][j]);
        }
        printf("\n");
    }
    
    printf("\nArray 3 Dimensi (2x2x2):\n");
    for(int i = 0; i < 2; i++) {
        printf("Layer %d:\n", i+1);
        for(int j = 0; j < 2; j++) {
            for(int k = 0; k < 2; k++) {
                printf("%d ", array3D[i][j][k]);
            }
            printf("\n");
        }
    }
    
    return 0;
}
